turn agendaObject\n        },\n\n    },\n    methods: {\n        // ...mapActions('threads', ['createThread']),\n    },\n    created() {\n        // let ms = Math.floor(Date.now() / 1000);\n        // console.log(ms)\n        // this.today = ms\n        // console.log(ms);\n        // let date_string = new Date (1594122308).toDateString()\n        // console.log(date_string)\n    }\n\n    \n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module"}